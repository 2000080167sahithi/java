class Point{
   int x; int y;
   void setX(int x){
   x=(x>79 ? 79 : (x<0?0:x));}
   void setY(int y){
   x=(x>24 ? 24 : (y<0?0:y));}
   int getX(){ return x;}
   int getY(){ return y;}
  }
class PointDemo
{
  public static void main(String args[]){
  Point p1=new Point();
  p1.x=22;
  p1.y=44;
  System.out.println("the value of x is"+p1.x);
  System.out.println("the value of y is"+p1.y);
}
}
   
